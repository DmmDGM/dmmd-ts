{
    "devDependencies": {
        "@types/bun": "latest"
    },
    "name": "dmmd-ts",
    "peerDependencies": {
        "typescript": "^5.8.2"
    },
    "scripts": {
        "build": "bun run buildcjs; bun run buildesm",
        "buildcjs": "bun build ./index.ts --outfile ./index.cjs --minify --format=cjs",
        "buildesm": "bun build ./index.ts --outfile ./index.mjs --minify --format=esm"
    },
    "type": "module"
}
